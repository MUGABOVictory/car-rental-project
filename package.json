{
  "name": "car-rental-app",
  "version": "1.0.0",
  "description": "Car Rental Service with MySQL backend and DevOps monitoring",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "test": "jest tests/utils.test.js tests/api.test.js --runInBand",
    "test:watch": "jest --watch",
    "test:integration": "jest tests/integration.test.js --runInBand --testTimeout=30000",
    "test:all": "npm run test && npm run test:integration",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "express": "^4.18.2",
    "mysql2": "^3.6.5"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.4"
  },
  "keywords": [
    "nodejs",
    "express",
    "mysql",
    "devops",
    "car-rental",
    "rental-management"
  ],
  "author": "DevOps Student",
  "license": "MIT"
}
